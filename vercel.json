{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/front-control-inventario"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/(.*)",
      "dest": "https://backend-control-inventario-desplegar.onrender.com/api/$1"
    },
    {
      "src": "/ws",
      "dest": "https://backend-control-inventario-desplegar.onrender.com/ws"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}